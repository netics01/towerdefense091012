<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="PlanetTerror.Enemy1"
	x:Name="UserControl" Width="50" Height="30">
	<UserControl.Resources>
		<Storyboard x:Key="Move_Storyboard" RepeatBehavior="Forever"/>
	</UserControl.Resources>

	<Canvas x:Name="LayoutRoot">
		<VisualStateManager.VisualStateGroups>
			<VisualStateGroup x:Name="Enemy_StateGroup">
				<VisualStateGroup.Transitions>
					<VisualTransition GeneratedDuration="00:00:00"/>
				</VisualStateGroup.Transitions>
				<VisualState x:Name="Move_Storyboard"/>
				<VisualState x:Name="Enemy_Hit_State"/>
				<VisualState x:Name="Enemy_Destroy_state"/>
				<VisualState x:Name="Enemy_Boom_state"/>
			</VisualStateGroup>
		</VisualStateManager.VisualStateGroups>
	</Canvas>
</UserControl>