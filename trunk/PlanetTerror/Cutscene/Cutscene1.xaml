													<RotateTransform/>
													<TranslateTransform Y="-0.12"/>
												</TransformGroup>
											</Rectangle.RenderTransform>
										</Rectangle>
									</DockPanel>
								</Border>
							</Border>
						</Border>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
			<Style.Triggers>
				<Trigger Property="Orientation" Value="Vertical">
